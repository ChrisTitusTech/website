.content th {
  font-weight: 500; }

.content thead {
  background: #FBFBFB;
  margin-bottom: 0; }
  .content thead tr {
    border-bottom: 1px solid #EEEEEE; }

.content tbody {
  background: #fff;
  margin-bottom: 0; }

.content .notices {
  margin: 2rem 0;
  position: relative;
  overflow: hidden; }

.content .notices p {
  padding: 10px;
  margin-bottom: 0; }

.content .notices.note p {
  border-top: 30px solid #6ab0de;
  background: #e7f2fa; }

.content .notices.note p::after {
  content: 'Note';
  position: absolute;
  top: 2px;
  color: #fff;
  left: 2rem; }

.content .notices.tip p {
  border-top: 30px solid #78C578;
  background: #E6F9E6; }

.content .notices.tip p::after {
  content: 'Tip';
  position: absolute;
  top: 2px;
  color: #fff;
  left: 2rem; }

.content .notices.info p {
  border-top: 30px solid #F0B37E;
  background: #FFF2DB; }

.content .notices.info p::after {
  content: 'Info';
